@{
    ViewBag.Title = "Home Page";
}
<div class="countDown">
    <img src="/Content/QR.png" />
    <h3>Or visit <a href="http://dh2015.azurewebsites.net">http://dh2015.azurewebsites.net</a></h3>
    <h1>Release your Anger</h1>
    <h1 id="countDown">
    </h1>
    <button class="start" type="button">Start</button>
</div>
<div id="divtreemapcontainer">
    <div class="treemap"></div>
</div>
<audio id="audiotag1" preload="auto">
    <source src="data:audio/wav;base64," />
</audio>
<audio id="audiotag2 preload="auto">
    <source src="data:audio/wav;base64," />
</audio>
<div class="result">
</div>

    <audio id="music" preload="auto">
        @*<source src="~/Content/Player.mp3" />*@
    </audio>


    @section scripts {
        <script src="/signalr/hubs"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.js"></script>
        <script>
            var leaderboard = [];
            function addKapow(){
                var img = $('<img src="/Content/Kapow.png" class="kapow" />');
                img.css('position', 'absolute');
                img.css('left', (window.screen.width-300)*Math.random()+'px');
                img.css('top', (window.screen.height-300)*Math.random()+ 'px');
                var g = 1;
                if (Math.random() > 0.5)
                {
                    g = -1;
                }
                img.css('transform','rotate('+Math.random()*50*g+'deg)');
                $('body').append(img);
            }


            var data = {
                "name": "cluster",
                "children": [
                ]
            };



            var color = d3.scale.category20c();

            function getLabel(d) {
                return d.name + "(B: " + d.mcap + " L: "+ d.last+ " A: " + d.amount+")";
            }

            function position() {
                this
                  .style("left", function (d) { return d.x + "%"; })
                  .style("top", function (d) { return d.y + "%"; })
                  .style("width", function (d) { return d.dx + "%"; })
                  .style("height", function (d) { return d.dy + "%"; });
            }


            $(document).ready(function () {
                var scoreboard = $.connection.scoreboardHub;
                var audio = $('#audiotag1');
                var audio2 = $('#audiotag2');
                var audio3 = $('#audiotag3');


                function init()
                {
                    scoreboard.server.getAllUsers().done(function (users) {
                        console.log(users);
                    })
                }

                scoreboard.client.updateUserBeats = function(userBeats)
                {
                    console.log(userBeats);
                }

                scoreboard.client.addPow = function()
                {
                    addKapow();
                    audio[0].play();
                }

                scoreboard.client.updateScoreboard = function (scoreboard) {
                    console.log(scoreboard);
                    leaderboard = scoreboard;
                    var data = {
                        "name": "cluster",
                        "children": scoreboard
                    };

                    d3.select(".treemap").selectAll(".node").remove();
                    //$("#divtreemapcontainer").append('<div class="treemap" />');

                    var div = d3.select(".treemap");
                    var treemap =
                d3.layout.treemap()
                // use 100 x 100 px, which we'll apply as % later
                .size([100, 100])
                        .sort(function (a, b) {
                            return a.value - b.value;
                        })
                .sticky(false)
                .value(function (d) { return d.mcap; });

                    var node =
                            div.datum(data).selectAll(".node")
                              .data(treemap.nodes)
                              .enter().append("div")
                              .attr("class", "node")
                              .call(position)
                              .style("background", function (d) { return color(d.name); })
                              .text(getLabel);
                    //console.log(data);

                }

                function start() {
                    $('.countDown').show();
                    $('#result').remove();
                    $('.kapow').remove();
                    //audio[0].play();
                    //audio2[0].play()
                    var countDown = 4;
                    var countDownFunc = function() {
                        countDown--;
                        if (countDown==0)
                        {
                            $('#countDown').text('GO');
                            $.ajax({
                                url: '/api/manage',
                                method: 'POST'
                            }).done(function() {
                                var music = $('#music')[0];
                                scoreboard.server.start();
                                music.play();
                                $('.countDown').hide();
                                countDown = 4;
                                $('#countDown').text('');
                                setTimeout(updateProgress, 100) ;
                            });
                        }
                        else
                        {
                            $('#countDown').text(countDown);
                            setTimeout(countDownFunc,1000)
                        }

                    };
                    setTimeout(countDownFunc,1000)

                };

                function showResult()
                {
                    function compare(a, b) {
                        if (a.mcap < b.mcap)
                            return 1;
                        if (a.mcap > b.mcap)
                            return -1;
                        return 0;
                    }

                    leaderboard.sort(compare);

                    var runnerUp = function() {
                        if (leaderboard.length>1)
                        {
                            return '<h3>Runner up: ' + leaderboard[1].name + '</h3>';
                        }
                        return "";
                    }

                    var getLeader = function ()
                    {
                        if (leaderboard.length==0)
                        {
                            return { name: 'No one', mcap: 0 };
                        }
                        var best = leaderboard[0];
                        return best;
                    }

                    var leader = getLeader();
                    var div = ''+
                        '<div id="result">'+
                            '<h1>The most angry</h1><h2>'+leader.name+
                            '</h2><h3>Anger released: ' + leader.mcap + '</h3>'+ runnerUp() +                            
                         '<div>'+
                    '<button class="start" type="button">Release more anger</button>'+
                '</div>'+
            '</div>';

                    var elm = $(div);
                    $('.start', elm).click(function() {
                        $('#result').remove();
                        $('.countDown').show();
                    });
                    $('.result').append(elm);
                }

                var roundTime  = 100; //Fix
                var currentTime = 0;
                function updateProgress() {
                    currentTime++;
                    $('footer > div').width((currentTime/roundTime)*100+'%');
                    if (currentTime < roundTime)
                    {
                        setTimeout(updateProgress,100);
                    }
                    else{
                        currentTime=0;
                        showResult();
                    }
                    
                };

                $('.start').click(start)

                // Start the connection
                $.connection.hub.start().done(init);
            });
        </script>
    }
